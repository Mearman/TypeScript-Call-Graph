{
    "name": "typescript-call-graph",
    "version": "0.0.3",
    "description": "Create a call graph of your TypeScript files",
    "keywords": [
        "TypeScript",
        "call graph",
        "callgraph"
    ],
    "contributors": [
        "Boris Yakubchik",
        "blake1821"
    ],
    "homepage": "https://yboris.dev",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/blake1821/TypeScript-Call-Graph.git"
    },
    "scripts": {
        "dev": "npm run clean && tsc -p src/server/tsconfig.json --watch & (npm run wait-for-compile && NODE_ENV=development nodemon --watch dist ./dist/src/server/index.js)",
        "start": "NODE_ENV=production node ./dist/src/server/index.js",
        "build": "npm run clean && tsc -p src/server/tsconfig.json && node ./dist/src/server/build.js",
        "test": "vitest",
        "clean": "rm -rf dist static",
        "wait-for-compile": "until [ -f dist/src/server/index.js ]; do sleep 1; done"
    },
    "dependencies": {
        "@trpc/client": "^10.45.2",
        "@trpc/server": "^10.45.2",
        "@types/cytoscape": "^3.21.3",
        "@types/cytoscape-fcose": "^2.2.4",
        "@types/d3-graphviz": "^2.6.10",
        "@types/express": "^4.17.21",
        "@types/graphviz": "^0.0.39",
        "@types/inquirer": "^9.0.7",
        "@types/node": "^18.11.3",
        "@types/open": "^6.2.1",
        "cytoscape": "^3.29.2",
        "cytoscape-fcose": "^2.2.0",
        "d3-graphviz": "^5.4.0",
        "dotenv": "^16.4.5",
        "events": "^3.3.0",
        "express": "^4.19.2",
        "glob": "^10.4.1",
        "graphviz": "^0.0.9",
        "inquirer": "^8.2.3",
        "kleur": "^4.1.5",
        "mermaid": "^9.4.3",
        "nodemon": "^3.1.3",
        "open": "^8.4.0",
        "ts-node": "^10.9.2",
        "vite": "^5.2.13",
        "vitest": "^1.6.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.4.0",
        "@types/eslint__js": "^8.42.3",
        "eslint": "^8.57.0",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.12.0"
    },
    "type": "module"
}
