{
  "name": "typescript-call-graph",
  "version": "0.0.1",
  "description": "Create a call graph of your TypeScript files",
  "keywords": [
    "TypeScript",
    "call graph"
  ],
  "author": "Boris Yakubchik",
  "homepage": "https://yboris.dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/whyboris/TypeScript-Call-Graph"
  },
  "main": "bin/index.js",
  "bin": {
    "tcg": "./bin/index.js"
  },
  "scripts": {
    "start": "tsc && mv index.js bin/index.js && mv extract.js bin/extract.js && node bin/index.js *.ts",
    "test": "node bin/index.js *.ts",
    "global": "npm install -g ."
  },
  "dependencies": {
    "@types/node": "14.0.11",
    "inquirer": "7.1.0",
    "kleur": "3.0.3",
    "typescript": "3.9.5"
  },
  "devDependencies": {}
}
