{
    "name": "typescript-call-graph",
    "version": "0.0.3",
    "description": "Create a call graph of your TypeScript files",
    "keywords": [
        "TypeScript",
        "call graph",
        "callgraph"
    ],
    "contributors": [
        "Boris Yakubchik",
        "blake1821"
    ],
    "homepage": "https://yboris.dev",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/blake1821/TypeScript-Call-Graph.git"
    },
    "scripts": {
        "dev": "NODE_ENV=development node --experimental-specifier-resolution=node --loader ts-node/esm ./src/server/index.ts",
        "start": "NODE_ENV=production node --experimental-specifier-resolution=node --loader ts-node/esm ./src/server/index.ts",
        "build": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/server/build.ts",
        "test": "jest"
    },
    "dependencies": {
        "@trpc/client": "^10.45.2",
        "@trpc/server": "^10.45.2",
        "@types/d3-graphviz": "^2.6.10",
        "@types/express": "^4.17.21",
        "@types/graphviz": "^0.0.39",
        "@types/inquirer": "^9.0.7",
        "@types/node": "^18.11.3",
        "@types/open": "^6.2.1",
        "d3-graphviz": "^5.4.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "glob": "^10.4.1",
        "graphviz": "^0.0.9",
        "inquirer": "^8.2.3",
        "jest": "^29.7.0",
        "kleur": "^4.1.5",
        "mermaid": "^9.4.3",
        "open": "^8.4.0",
        "vite": "^5.2.13",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.4.0",
        "@types/eslint__js": "^8.42.3",
        "eslint": "^8.57.0",
        "ts-jest": "^29.1.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.12.0"
    },
    "type": "module"
}